{
  "manifest_version": 3,
  "name": "Fuck Quora",
  "version": "1.0",
  "description": "Specifically omits Quora from all Google search requests.",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
